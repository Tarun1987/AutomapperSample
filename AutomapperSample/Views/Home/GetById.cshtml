@model AutomapperSample.Models.CustomerDto
@{
    ViewBag.Title = string.Format($"{Model.FirstName} {Model.LastName}");
}

<h2>@string.Format($"{Model.FirstName} {Model.LastName}")</h2>

<div class="card" style="width: 20rem;">
    <div class="card-body">
        <h4 class="card-title">@Model.Email</h4>
        <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6>
        <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
        <a href="#" class="card-link">@Model.DOB</a>
        <a href="#" class="card-link">@Model.Age</a>
    </div>
</div>

